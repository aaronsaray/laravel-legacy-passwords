{
    "name": "aaronsaray/laravel-legacy-passwords",
    "description": "Toolset to migrate legacy passwords to your current laravel project",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^7.1",
        "laravel/framework": ">=5.5"
    },
    "autoload": {
        "psr-4": {
            "AaronSaray\\LaravelLegacyPasswords\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AaronSaray\\LaravelLegacyPasswords\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AaronSaray\\LaravelLegacyPasswords\\ServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5 || ^8.0",
        "squizlabs/php_codesniffer": "^3.4",
        "mockery/mockery": "^1.2"
    },
    "scripts": {
        "test": [
            "@php vendor/bin/phpunit --colors=always"
        ],
        "phpcs": [
            "@php vendor/bin/phpcs"
        ]
    },
    "scripts-descriptions": {
        "test": "Run unit tests using PHPUnit",
        "phpcs": "Run PHP Code Sniffer"
    }
}
